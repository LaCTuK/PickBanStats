spring.jpa.hibernate.ddl-auto=none
spring.datasource.url=jdbc:mysql://127.0.0.1:3306/DBNAME?useSSL=false
spring.datasource.username=USERNAME
spring.datasource.password=PASSWORD

spring.thymeleaf.cache=false
spring.thymeleaf.enabled=true
spring.application.name=The Spammers ban page

management.endpoint.httptrace.enabled=true
management.endpoint.metrics.enabled=true
management.endpoint.info.enabled=true
management.endpoint.mappings.enabled=true
management.endpoint.health.enabled=true

management.endpoints.web.base-path=/adminstats
management.endpoints.web.path-mapping.httptrace=http

management.endpoints.web.exposure.include=httptrace, metrics, info, health, mappings